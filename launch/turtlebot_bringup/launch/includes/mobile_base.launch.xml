<!--
  The mobile platform base.
  
  Selector for the base.
 -->
<launch>
  <arg name="base"/>
  <arg name="serialport"/>
  <include file="$(find multi_agent)/launch/turtlebot_bringup/launch/includes/$(arg base)/mobile_base.launch.xml">
    <arg name="serialport" value="$(arg serialport)" />
  </include>
</launch>

